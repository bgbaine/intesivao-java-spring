1}]}]}},YFqc:function(e,a,o){e.exports=o("cTJO")},cTJO:function(e,a,o){"use strict";var s=o("J4zp"),r=o("284h");a.__esModule=!0,a.default=void 0;var i=r(o("q1tI")),t=o("elyg"),d=o("nOHt"),c=o("vNVm"),n={};function u(e,a,o,s){if(e&&(0,t.isLocalURL)(a)){e.prefetch(a,o,s).catch((function(e){0}));var r=s&&"undefined"!==typeof s.locale?s.locale:e&&e.locale;n[a+"%"+o+(r?"%"+r:"")]=!0}}var m=function(e){var a=!1!==e.prefetch,o=(0,d.useRouter)(),r=o&&o.pathname||"/",m=i.default.useMemo((function(){var a=(0,t.resolveHref)(r,e.href,!0),o=s(a,2),i=o[0],d=o[1];return{href:i,as:e.as?(0,t.resolveHref)(r,e.as):d||i}}),[r,e.href,e.as]),p=m.href,l=m.as,v=e.children,f=e.replace,g=e.shallow,b=e.scroll,q=e.locale;"string"===typeof v&&(v=i.default.createElement("a",null,v));var h=i.Children.only(v),S=h&&"object"===typeof h&&h.ref,z=(0,c.useIntersection)({rootMargin:"200px"}),A=s(z,2),j=A[0],P=A[1],x=i.default.useCallback((function(e){j(e),S&&("function"===typeof S?S(e):"object"===typeof S&&(S.current=e))}),[S,j]);(0,i.useEffect)((function(){var e=P&&a&&(0,t.isLocalURL)(p),s="undefined"!==typeof q?q:o&&o.locale,r=n[p+"%"+l+(s?"%"+s:"")];e&&!r&&u(o,p,l,{locale:s})}),[l,p,P,q,a,o]);var C={ref:x,onClick:function(e){h.props&&"function"===typeof h.props.onClick&&h.props.onClick(e),e.defaultPrevented||function(e,a,o,s,r,i,d,c){("A"!==e.currentTarget.nodeName||!function(e){var a=e.currentTarget.target;return a&&"_self"!==a||e.metaKey||e.ctrlKey||e.shiftKey||e.altKey||e.nativeEvent&&2===e.nativeEvent.which}(e)&&(0,t.isLocalURL)(o))&&(e.preventDefault(),null==d&&(d=s.indexOf("#")<0),a[r?"replace":"push"](o,s,{shallow:i,locale:c,scroll:d}).then((function(e){e&&d&&document.body.focus()})))}(e,o,p,l,f,g,b,q)},onMouseEnter:function(e){(0,t.isLocalURL)(p)&&(h.props&&"function"===typeof h.props.onMouseEnter&&h.props.onMouseEnter(e),u(o,p,l,{priority:!0}))}};if(e.passHref||"a"===h.type&&!("href"in h.props)){var V="undefined"!==typeof q?q:o&&o.locale,N=(0,t.getDomainLocale)(l,V,o&&o.locales,o&&o.domainLo